<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="464.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="com.example.group51_kurbanihut.Nahien.MarkFeedbackSolveController">

    <Label fx:id="titleLabel" layoutX="161.0" layoutY="14.0" text="Mark Feedback as Solved">
      <font>
         <Font size="25.0" />
      </font></Label>

    <TableView fx:id="feedbackTable" layoutX="20.0" layoutY="80.0" prefHeight="275.0" prefWidth="560">
        <columns>
            <TableColumn fx:id="feedbackColumn" prefWidth="380.79998779296875" text="Feedback" />
            <TableColumn fx:id="statusColumn" prefWidth="177.5999755859375" text="Status" />
        </columns>
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
    </TableView>

    <Button layoutX="220" layoutY="370" onAction="#solveFeedbackButton" text="Solve Selected Feedback" />
   <Button layoutX="531.0" layoutY="424.0" onAction="#previousButton" text="Previous" />

</AnchorPane>
