<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.group51_kurbanihut.Nahien.PaymentController">
   <children>
      <ComboBox fx:id="paymentMethodComboBox" layoutX="383.0" layoutY="100.0" prefWidth="150.0" promptText="Payment Method" />
      <Button layoutX="383.0" layoutY="147.0" mnemonicParsing="false" onAction="#makePaymentButton" prefHeight="26.0" prefWidth="150.0" text="Make Payment" />
      <Button layoutX="534.0" layoutY="360.0" mnemonicParsing="false" onAction="#previousButton" text="Previous" />
      <Label layoutX="187.0" layoutY="21.0" prefHeight="46.0" prefWidth="226.0" text="Make a Payment">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <TableView fx:id="cattleCol" layoutX="23.0" layoutY="100.0" prefHeight="200.0" prefWidth="340.0">
        <columns>
          <TableColumn fx:id="cattleIdCol" prefWidth="75.0" text="Cattle Id" />
          <TableColumn fx:id="cattlePriceCol" prefWidth="75.0" text="Cattle Price" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Label fx:id="label" layoutX="23.0" layoutY="318.0" prefHeight="71.0" prefWidth="340.0" />
   </children>
</AnchorPane>
